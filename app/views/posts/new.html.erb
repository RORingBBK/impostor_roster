<%= form_for @post do |f| %>
  <%= f.label "Field:" %>
  <%= f.text_field :field %></br>
  <%= f.label "Months of Experience:" %>
  <%= f.text_field :months_experience %></br>
  <%= f.label "Thing you are bad at:" %>
  <%= f.text_field :inability %></br>
  <%= f.submit %></br>
  <%= link_to "Go Back", posts_path %>
<% end %>


